{
  "name": "dynamic-express-boilerplate",
  "version": "1.0.0",
  "description": "A dynamic express boilerplate with TypeScript, Prisma, and JWT authentication. Includes Google OAuth2.0 and Passport.js for authentication, and uses PostgreSQL as the database.",
  "homepage": "https://github.com/Spooner8/dynamic-express-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/Spooner8/dynamic-express-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spooner8/dynamic-express-boilerplate.git"
  },
  "license": "MIT",
  "author": "Spooner8",
  "scripts": {
    "test": "test",
    "build": "tsc",
    "dev": "ts-node-dev ./src/app.ts",
    "prod": "npm run db:deploy && node ./dist/app.js",
    "db:deploy": "npx prisma migrate deploy && npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-prometheus-middleware": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "ts-node-dev": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-prometheus-middleware": "^1.2.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.11",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  }
}
